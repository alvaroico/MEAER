{
  "swagger": "2.0",
  "info": {
    "description": "Teste MEAER",
    "version": "0.0.1",
    "title": "Swagger MEAER",
    "contact": {
      "email": "alvaro.r.p@hotmail.com"
    },
    "license": {
      "name": "Apache 2.0",
      "url": "http://www.apache.org/licenses/LICENSE-2.0.html"
    }
  },
  "host": "localhost:8822",
  "basePath": "",
  "tags": [],
  "schemes": ["http"],
  "paths": {
    "/": {
      "get": {
        "produces": ["application/json"],
        "responses": {
          "200": {
            "description": "Rota Home"
          }
        },
        "security": [
          {
            "petstore_auth": ["write:pets", "read:pets"]
          }
        ]
      }
    },
    "/senha": {
      "get": {
        "parameters": [
          {
            "name": "Authorization",
            "in": "header",
            "description": "JWT access token",
            "required": true,
            "type": "string"
          }
        ],
        "produces": ["application/json"],
        "responses": {
          "200": {
            "description": "Rota Senha"
          },
          "401": {
            "description": "Não identificado Token"
          }
        }
      }
    },
    "/senha/pass": {
      "post": {
        "parameters": [
          {
            "name": "Authorization",
            "in": "header",
            "description": "JWT access token",
            "required": true,
            "type": "string"
          },
          {
            "name": "Senha",
            "in": "body",
            "description": "senha",
            "required": true,
            "type": "string",
            "schema": {
              "type": "object",
              "properties": {
                "senha": {
                  "type": "string"
                }
              }
            }
          }
        ],
        "produces": ["application/json"],
        "responses": {
          "200": {
            "description": "Retorno do Objeto Senha",
            "content": ["application/json"],
            "schema": {
              "type": "object",
              "properties": {
                "pass": {
                  "type": "boolean"
                }
              }
            }
          },
          "400": {
            "description": "Senha não informado. | Senha não atendo o minimo carácter."
          },
          "401": {
            "description": "Não identificado Token"
          }
        }
      }
    }
  }
}
